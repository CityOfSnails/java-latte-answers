public class FindMinimum extends ConsoleProgram
{
    public void run()
    {
        int num1 = readInt("Enter first number: ");
        int num2 = readInt("Enter second number: ");
        int num3 = readInt("Enter third number: ");
        
        if(num1 - num2 < 0 && num1 - num3 < 0)
        {
            System.out.println(num1);
        }
        
        if(num2 - num1 < 0 && num2 - num3 < 0)
        {
            System.out.println(num2);
        }
        
        if(num3 - num1 < 0 && num3 - num2 < 0)
        {
            System.out.println(num3);
        }
        
        else
        {
            System.out.println(num1);
        }
    }
}
